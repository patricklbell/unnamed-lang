fibonacci: (x: int) -> int = {
	if x <= 0 {
		return 0;
	}
	if x == 1 {
		return 1;
	}

	return fibonacci(x - 1) + fibonacci(x - 2);
}

main: () -> int = {
	print_alphabet();
	return 0;
}

putchar: (c: int) -> int;

print_alphabet: () -> void = {
	i := 97;
	while i <= 122 {
		putchar(i);
		*i = i + 1;
	}
	putchar(10);
}
